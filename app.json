{
  "expo": {
    "name": "react-native-posts",
    "slug": "react-native-posts",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#222736"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.alksndr.PostsReactNative",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.alksndr.PostsReactNative",
      "versionCode": 1,
      "permissions": ["CAMERA", "WRITE_EXTERNAL_STORAGE"]
    }
  }
}
